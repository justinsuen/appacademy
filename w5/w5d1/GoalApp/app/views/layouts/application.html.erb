<!DOCTYPE html>
<html>
<head>
  <title>GoalApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

<% if logged_in? %>
  <%= link_to "Log out", session_url, method: :delete %>
<% else %>
  <%= link_to "Log in", new_session_url, method: :new %>
  <%= link_to "Sign up", new_user_url, method: :new %>
<% end %>
</body>
</html>
